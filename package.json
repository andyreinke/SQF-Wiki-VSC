{
    "name": "sqf-wiki",
    "displayName": "SQF Wiki",
    "description": "Want to look up some SQF functions? Now you can, with just a single click!",
    "version": "0.0.1",
    "publisher": "EelisLynne",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showWiki"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showWiki",
                "title": "Show on SQF Wiki"
            }
        ],
        "menus": {
            "editor/context": [{
                "command": "extension.showWiki",
                "group": "1_modification",
                "when": "editorLangId == sqf"
            }]
        },
        "keybindings": [{
            "command": "extension.showWiki",
            "key": "ctrl+shift+q",
            "mac": "cmd+shift+q",
            "when": "editorLangId == sqf"
        }]
    },
    
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
